import java.util.*;
class pattern
{
	public static void main(String[] args)
	{
		Scanner input=new Scanner(System.in);
		int n=5;
		for(int i=1;i<=n;i++)
		{
			for(int j=5;j>=i;j--)
			{
				System.out.print(" ");
			}
			for(int k=1;k<=n;k++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
